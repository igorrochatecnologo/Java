package tiposprimitivos;


public class TiposPrimitivos {

   
    public static void main(String[] args) {
        
    float nota = 8.5f;
    System.out.println("A nota é " + nota);
    }
    
    
}



---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

package tiposprimitivos;


public class TiposPrimitivos {

   
    public static void main(String[] args) {
    String nome = "Igor";    
    float nota = 8.5f;
    System.out.printf("A nota de %s é %.1f \n", nome, nota);
    }
    
    
}






------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
